public class c {
int[] a={0, 0, 0, 0, 0, 0};
int dep=0;
public int dfs(int n, int m) {
int i=0;
if (((n==0)&&(m==0)))
{
while ((i<this.dep))
{
putnum(this.a[i]);
putch(32);
i=(i+1);
}
putch(10);
return 0;
}
if (((n<=0)||(m==0)))
return 0;
this.dep=(this.dep+1);
i=1;
while ((i<=n))
{
this.a[(this.dep-1)]=i;
this.dfs((n-i), (m-1));
